generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id             String   @id @default(uuid()) @db.VarChar(255)
  name           String?
  hashedPassword String?
  createdAt      DateTime @default(now()) @db.Timestamp(6)
  email          String   @unique
  image          String?
}
